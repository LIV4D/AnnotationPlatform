{
  "name": "server",
  "description": "Projet LIV4D",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tsc && NODE_ENV=production node out/www.js",
    "start-windows": "tsc && SET NODE_ENV=production && node out/www.js",
    "dev": "concurrently --kill-others \"tsc -w \" \"NODE_ENV=development nodemon out/server/app/www.js \"",
    "lint": "tslint -c tslint.json --format stylish app/**/*.ts test/**/*.ts",
    "test": "NODE_ENV=test mocha -r ts-node/register test/**/*.spec.ts --timeout 3000",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "postinstall": "rimraf false",
    "seeding-standard": "NODE_ENV=standard ts-node -T seeding/loadSeeds.ts",
    "seeding-standard-windows": "SET NODE_ENV=standard && ts-node -T seeding/loadSeeds.ts",
    "seeding-test": "NODE_ENV=test ts-node -T seeding/loadSeeds.ts"
  },
  "dependencies": {
    "@types/sharp": "^0.24.0",
    "@types/tmp": "^0.1.0",
    "body-parser": "1.18.3",
    "class-validator": "0.9.1",
    "config": "3.0.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "crypto-js": "3.1.9-1",
    "debug": "4.1.0",
    "express": "4.16.3",
    "google-spreadsheet": "^2.0.6",
    "inversify": "5.0.1",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "nodemon": "1.18.7",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.6.1",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.5.0",
    "sharp": "^0.24.1",
    "tmp": "^0.1.0",
    "ts-node": "7.0.1",
    "typeorm": "0.2.7",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.6",
    "@types/chai-http": "3.0.5",
    "@types/chai-things": "0.0.33",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/crypto-js": "3.1.43",
    "@types/express": "4.16.0",
    "@types/express-serve-static-core": "4.16.0",
    "@types/mocha": "5.2.5",
    "@types/morgan": "1.7.35",
    "@types/multer": "1.3.7",
    "@types/node": "10.12.10",
    "@types/passport": "0.4.6",
    "@types/passport-jwt": "3.0.1",
    "@types/passport-local": "1.0.33",
    "@types/request-promise-native": "1.0.15",
    "@types/xml2js": "0.4.3",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "chai-things": "0.2.0",
    "concurrently": "4.1.0",
    "mocha": "5.2.0",
    "rimraf": "2.6.2",
    "sqlite3": "4.0.2",
    "tslint": "5.11.0",
