{
  "name": "server",
  "description": "Projet LIV4D",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tsc && NODE_ENV=production node out/www.js",
    "start-windows": "concurrently --kill-others \"tsc -w \" \"SET NODE_ENV=development && nodemon out/server/app/www.js\"",
    "dev": "concurrently --kill-others \"tsc -w \" \"NODE_ENV=development nodemon out/server/app/www.js \"",
    "lint": "tslint -c tslint.json --format stylish app/**/*.ts test/**/*.ts",
    "test": "NODE_ENV=test mocha -r ts-node/register test/**/*.spec.ts --timeout 3000",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "postinstall": "rimraf false",
    "seeding-standard": "NODE_ENV=standard ts-node -T seeding/loadSeeds.ts",
    "seeding-standard-windows": "SET NODE_ENV=standard && ts-node -T seeding/loadSeeds.ts",
    "seeding-test": "NODE_ENV=test ts-node -T seeding/loadSeeds.ts"
  },
  "dependencies": {
    "@types/sharp": "^0.24.0",
    "@types/tmp": "^0.1.0",
    "body-parser": "1.19.0",
    "class-validator": "0.11.0",
    "config": "3.3.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "crypto-js": "4.0.0",
    "debug": "4.1.1",
    "express": "4.17.1",
    "google-spreadsheet": "^3.0.9",
    "inversify": "5.0.1",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "nodemon": "2.0.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.18.2",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "serve-favicon": "2.5.0",
    "sharp": "^0.24.1",
    "tmp": "^0.1.0",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.24",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.9",
    "@types/chai-http": "4.2.0",
    "@types/chai-things": "0.0.34",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/crypto-js": "3.1.43",
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.2",
    "@types/mocha": "7.0.1",
    "@types/morgan": "1.9.0",
    "@types/multer": "1.4.2",
    "@types/node": "13.7.7",
    "@types/passport": "1.0.2",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/request-promise-native": "1.0.17",
    "@types/xml2js": "0.4.5",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "chai-things": "0.2.0",
    "concurrently": "5.1.0",
    "mocha": "7.1.0",
    "rimraf": "3.0.2",
    "sqlite3": "4.1.1",
    "tslint": "6.0.0",
